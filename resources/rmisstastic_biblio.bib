@Article{stubbendick_ibrahim_2003,
  Title                    = {Maximum Likelihood Methods for Nonignorable Missing Responses and Covariates in Random Effects Models},
  Author                   = {Stubbendick, A. L. and Ibrahim, J. G.},
  Journal                  = {Biometrics},
  Pages                    = {1140--1150},
