}

@Article{josse_etal_JSFdS2009,
  Title                    = {Gestion des donn\'ees manquantes en {A}nalyse en {C}omposantes {P}rincipales},
  Author                   = {Josse, J. and Husson, F. and Pag\`es, J.},
